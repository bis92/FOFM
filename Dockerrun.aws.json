{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "bis92/fofm-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
            
        },
        {
            "name": "server",
            "image": "bis92/fofm-server",
            "hostname": "server",
            "essential": false,
            "memory": 700
        },
        {
            "name": "nginx",
            "image": "bis92/fofm-nginx",
            "hostname": "nginx",
            "essential": true,
            "memory": 128,
            "links": ["client", "server"]
        }
    ]
}